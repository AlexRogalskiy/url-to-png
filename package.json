{
  "name": "@jmondi/url-to-png",
  "description": "URL to PNG Server",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/*",
    "serve": "node -r dotenv/config dist/main.js",
    "dev": "nodemon",
    "compile": "tsc",
    "build": "run-s clean compile",
    "pretty": "prettier --single-quote --print-width 100 --parser typescript --trailing-comma es5 --write src/**/*.ts build/**/*.js"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 5.8.0"
  },
  "engineStrict": true,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/core": "^7.0.9",
    "@nestjs/platform-express": "^7.0.9",
    "aws-sdk": "^2.672.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.3",
    "generic-pool": "^3.7.1",
    "helmet": "^3.22.0",
    "md5": "^2.2.1",
    "nano": "^8.2.2",
    "puppeteer": "^3.0.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "sharp": "^0.25.2",
    "valid-url": "^1.0.9",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/generic-pool": "^3.1.9",
    "@types/node": "^10.17.21",
    "@types/puppeteer": "^1.20.5",
    "cross-env": "^5.2.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3"
  }
}

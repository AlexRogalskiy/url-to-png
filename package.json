{
  "name": "@jmondi/recipes",
  "description": "Webpack Starter Pack",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/*",
    "build:dev": "npm run clean && webpack --config build/webpack.conf.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config build/webpack.conf.js",
    "pretty": "prettier --write src/**/*.ts src/**/*.scss"
  },
  "engines": {
    "node": ">= 9.11.0",
    "npm": ">= 5.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "nunjucks": "^3.1.3",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/helper-plugin-utils": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "@types/nunjucks": "^3.0.0",
    "babel-loader": "^8.0.0-beta.4",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.1.6",
    "css-loader": "~0.28.11",
    "html-webpack-plugin": "~3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-loader": "^2.1.6",
    "purgecss-webpack-plugin": "^1.2.0",
    "style-loader": "~0.21.0",
    "tailwindcss": "^0.5.3",
    "ts-loader": "~4.2.0",
    "typescript": "^2.8.4",
    "webpack": "~4.6.0",
    "webpack-cli": "~2.0.15"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "typescript"
  }
}

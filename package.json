{
  "name": "@jmondi/url-to-png",
  "description": "URL to PNG Server",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/*",
    "serve": "node  -r dotenv/config dist/main.js",
    "dev": "nodemon",
    "compile": "tsc",
    "build": "run-s clean compile",
    "pretty": "prettier --single-quote --print-width 100 --parser typescript --trailing-comma es5 --write src/**/*.ts build/**/*.js"
  },
  "engines": {
    "node": ">= 9.11.0",
    "npm": ">= 5.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "aws-sdk": "^2.285.1",
    "dotenv": "^6.0.0",
    "generic-pool": "^3.4.2",
    "md5": "^2.2.1",
    "nano": "^7.0.0",
    "puppeteer": "^1.6.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "sharp": "^0.20.5"
  },
  "devDependencies": {
    "@types/node": "^10.5.5",
    "@types/puppeteer": "^1.6.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}

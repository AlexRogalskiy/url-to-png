{
  "name": "@jmondi/recipes",
  "description": "Webpack Starter Pack",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/*",
    "serve": "ts-node src/main.ts",
    "dev": "run-s clean build:dev",
    "prod": "run-s clean build:prod",
    "build:dev": "webpack --config build/webpack.conf.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config build/webpack.conf.js"
  },
  "engines": {
    "node": ">= 9.11.0",
    "npm": ">= 5.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "md5": "^2.2.1",
    "nano": "^7.0.0",
    "puppeteer": "^1.6.1",
    "reflect-metadata": "^0.1.12",
    "sharp": "^0.20.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/helper-plugin-utils": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "@types/node": "^10.5.4",
    "babel-loader": "^8.0.0-beta.4",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  }
}

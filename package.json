{
  "name": "@jmondi/url-to-png",
  "version": "1.3.1",
  "description": "URL to PNG Server",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/*",
    "serve": "node -r dotenv/config dist/main.js",
    "dev": "nodemon",
    "compile": "tsc",
    "build": "run-s clean compile",
    "format": "prettier --write \"./src/**/*.ts\""
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 5.8.0"
  },
  "engineStrict": true,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@nestjs/platform-fastify": "^7.6.18",
    "aws-sdk": "^2.1073.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "dotenv": "^8.6.0",
    "fastify-helmet": "^5.3.2",
    "fastify-rate-limit": "^5.7.1",
    "generic-pool": "^3.8.2",
    "md5": "^2.3.0",
    "nano": "^9.0.5",
    "playwright": "^1.19.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "sharp": "^0.30.1",
    "winston": "^3.6.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/generic-pool": "^3.1.10",
    "@types/node": "^14.18.11",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  }
}
